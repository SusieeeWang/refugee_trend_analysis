# Load necessary libraries\
library(readxl)\
library(dplyr)\
library(tidyr)\
\
# Define the file path\
file_path <- "your_file.xlsx"  # Replace with your file path\
\
# Step 1: Read all sheets into a list\
sheet_names <- excel_sheets(file_path)\
sheet_data <- lapply(sheet_names, function(sheet) \{\
  read_excel(file_path, sheet = sheet)\
\})\
\
# Step 2: Merge sheets by adding columns to match the same variable names\
# Ensure the column names are consistent across sheets\
merged_data <- sheet_data[[1]]  # Start with the first sheet as the base\
for (i in 2:length(sheet_data)) \{\
  # Align the columns by variable names and bind the columns side by side\
  merged_data <- left_join(merged_data, sheet_data[[i]], by = "Variable Name", suffix = c("", paste0("_Sheet", i)))\
\}\
\
# Step 3: View the merged data\
print(merged_data)\
\
# Step 4: Save the result to a new Excel file\
write.xlsx(merged_data, file = "merged_output.xlsx")\
\
print("Merged data has been saved to 'merged_output.xlsx'")\
