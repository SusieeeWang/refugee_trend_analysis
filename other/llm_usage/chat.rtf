\f5\fs22 \cf3 \cb4 \strokec3 \'b8\'b4\'d6\'c6\'b4\'fa\'c2\'eb
\f4\fs24 \cf2 \cb1 \strokec2 \
# Initialize an empty data frame to store the results\
selected_values <- data.frame()\
\
# Number of rows\
n_rows <- 78\
\
# Length of the cycle\
cycle_length <- 7\
\
# Iterate over each cycle\
for (i in 0:(ncol(data) %/% cycle_length - 1)) \{\
  # Columns to select in this cycle (skip first column)\
  col_indices <- c(2 + i * cycle_length, 5 + i * cycle_length)\
  \
  # Extract the selected columns\
  selection <- data[1:n_rows, col_indices]\
  \
  # Combine the values by stacking them vertically\
  selected_values <- rbind(selected_values, selection)\
\}\
\
# Rename columns for clarity (optional)\
colnames(selected_values) <- c("C2", "C5")\
\
# View the resulting stacked data\
print(selected_values)\
