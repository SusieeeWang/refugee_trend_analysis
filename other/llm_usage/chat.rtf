\f5\fs22 \cf3 \cb4 \strokec3 \'b8\'b4\'d6\'c6\'b4\'fa\'c2\'eb
\f4\fs24 \cf2 \cb1 \strokec2 \
# Install and load ggplot2\
if (!require("ggplot2")) install.packages("ggplot2")\
library(ggplot2)\
\
# Example Data\
data <- data.frame(\
  x = 1:10,  # X-axis values\
  y = c(2, 5, 3, 8, 6, 7, 9, 10, 12, 11)  # Y-axis values\
)\
\
# Create a line graph\
ggplot(data, aes(x = x, y = y)) +\
  geom_line(color = "blue", size = 1.2) +  # Add the line\
  geom_point(color = "red", size = 3) +   # Add points\
  labs(title = "Line Graph", x = "X-axis", y = "Y-axis") +  # Add labels\
  theme_minimal()  # Use a minimal theme\
